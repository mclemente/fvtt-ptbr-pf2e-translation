  "mapping": {
    "crew": "system.details.crew",
    "description": {
      "converter": "translateActorDescription",
      "path": "translateActorDescription"
    },
    "items": {
      "converter": "translateActorItems",
      "path": "items"
    },
    "name": {
      "converter": "translateDualLanguage",
      "path": "name"
    },
    "pilotingCheck": "system.details.pilotingCheck",
